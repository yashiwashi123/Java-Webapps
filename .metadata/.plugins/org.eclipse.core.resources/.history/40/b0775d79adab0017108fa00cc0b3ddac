package vector;

public class PVector {
	public double x;
	public double y;
	
	public PVector() {
		x = 0;
		y = 0; 
	}
	
	public PVector(double x, double y) {
		this.x = x; 
		this.y = y;
	
	}
	public void add(PVector v) {
		x = v.x +x; 
		y = v.y +y; 
	}
	public void sub(PVector v) {
		x = x - v.x; 
		y = y - v.y; 
	}
	public void mult(double f) {
		x = x*f;
		y = y*f; 
	}
	public double mag() {
		double mag = Math.sqrt((this.x*this.x) + (this.y*this.y));
		return mag; 
	}
	public void normalize() {
		
	}
}
